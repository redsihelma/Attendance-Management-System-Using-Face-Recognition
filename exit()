[32m+[m[32mdef update_attendance_in_db_in(present):[m
[32m+[m	[32mtoday=datetime.date.today()[m
[32m+[m	[32mtime=datetime.datetime.now()[m
[32m+[m	[32mfor person in present:[m
[32m+[m		[32muser=User.objects.get(username=person)[m
[32m+[m		[32mtry:[m
[32m+[m			[32mqs=Present.objects.get(user=user,date=today)[m
[32m+[m		[32mexcept:[m
[32m+[m			[32mqs= None[m
[32m+[m[41m		[m
[32m+[m		[32mif qs is None:[m
[32m+[m			[32mif present[person]==True:[m
[32m+[m						[32ma=Present(user=user,date=today,present=True)[m
[32m+[m						[32ma.save()[m
[32m+[m			[32melse:[m
[32m+[m				[32ma=Present(user=user,date=today,present=False)[m
[32m+[m				[32ma.save()[m
[32m+[m		[32melse:[m
[32m+[m			[32mif present[person]==True:[m
[32m+[m				[32mqs.present=True[m
[32m+[m				[32mqs.save(update_fields=['present'])[m
[32m+[m		[32mif present[person]==True:[m
[32m+[m			[32ma=Time(user=user,date=today,time=time, out=False)[m
[32m+[m			[32ma.save()[m
[32m+[m
[32m+[m
[32m+[m[41m			[m
[32m+[m[41m		[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mdef update_attendance_in_db_out(present):[m
[32m+[m	[32mtoday=datetime.date.today()[m
[32m+[m	[32mtime=datetime.datetime.now()[m
[32m+[m	[32mfor person in present:[m
[32m+[m		[32muser=User.objects.get(username=person)[m
[32m+[m		[32mif present[person]==True:[m
[32m+[m			[32ma=Time(user=user,date=today,time=time, out=True)[m
[32m+[m			[32ma.save()[m
[32m+[m[41m		[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mdef check_validity_times(times_all):[m
[32m+[m
[32m+[m	[32mif(len(times_all)>0):[m
[32m+[m		[32msign=times_all.first().out[m
[32m+[m	[32melse:[m
[32m+[m		[32msign=True[m
[32m+[m	[32mtimes_in=times_all.filter(out=False)[m
[32m+[m	[32mtimes_out=times_all.filter(out=True)[m
[32m+[m	[32mif(len(times_in)!=len(times_out)):[m
[32m+[m		[32msign=True[m
[32m+[m	[32mbreak_hourss=0[m
[32m+[m	[32mif(sign==True):[m
[32m+[m			[32mcheck=False[m
[32m+[m			[32mbreak_hourss=0[m
[32m+[m			[32mreturn (check,break_hourss)[m
[32m+[m	[32mprev=True[m
[32m+[m	[32mprev_time=times_all.first().time[m
[32m+[m
[32m+[m	[32mfor obj in times_all:[m
[32m+[m		[32mcurr=obj.out[m
[32m+[m		[32mif(curr==prev):[m
[32m+[m			[32mcheck=False[m
[32m+[m			[32mbreak_hourss=0[m
[32m+[m			[32mreturn (check,break_hourss)[m
[32m+[m		[32mif(curr==False):[m
[32m+[m			[32mcurr_time=obj.time[m
[32m+[m			[32mto=curr_time[m
[32m+[m			[32mti=prev_time[m
[32m+[m			[32mbreak_time=((to-ti).total_seconds())/3600[m
[32m+[m			[32mbreak_hourss+=break_time[m
[32m+[m
[32m+[m
[32m+[m		[32melse:[m
[32m+[m			[32mprev_time=obj.time[m
[32m+[m
[32m+[m		[32mprev=curr[m
[32m+[m
[32m+[m	[32mreturn (True,break_hourss)[m
[32m+[m
[32m+[m
[32m+[m[32mdef convert_hours_to_hours_mins(hours):[m
[32m+[m[41m	[m
[32m+[m	[32mh=int(hours)[m
[32m+[m	[32mhours-=h[m
[32m+[m	[32mm=hours*60[m
[32m+[m	[32mm=math.ceil(m)[m
[32m+[m	[32mreturn str(str(h)+ " hrs " + str(m) + "  mins")[m
[32m+[m
[32m+[m[41m		[m
[32m+[m
[32m+[m[32m#used[m
[32m+[m[32mdef hours_vs_date_given_employee(present_qs,time_qs,admin=True):[m
[32m+[m	[32mregister_matplotlib_converters()[m
[32m+[m	[32mdf_hours=[][m
[32m+[m	[32mdf_break_hours=[][m
[32m+[m	[32mqs=present_qs[m
[32m+[m
[32m+[m	[32mfor obj in qs:[m
[32m+[m		[32mdate=obj.date[m
[32m+[m		[32mtimes_in=time_qs.filter(date=date).filter(out=False).order_by('time')[m
[32m+[m		[32mtimes_out=time_qs.filter(date=date).filter(out=True).order_by('time')[m
[32m+[m		[32mtimes_all=time_qs.filter(date=date).order_by('time')[m
[32m+[m		[32mobj.time_in=None[m
[32m+[m		[32mobj.time_out=None[m
[32m+[m		[32mobj.hours=0[m
[32m+[m		[32mobj.break_hours=0[m
[32m+[m		[32mif (len(times_in)>0):[m[41m			[m
[32m+[m			[32mobj.time_in=times_in.first().time[m
[32m+[m[41m			[m
[32m+[m		[32mif (len(times_out)>0):[m
[32m+[m			[32mobj.time_out=times_out.last().time[m
[32m+[m
[32m+[m		[32mif(obj.time_in is not None and obj.time_out is not None):[m
[32m+[m			[32mti=obj.time_in[m
[32m+[m			[32mto=obj.time_out[m
[32m+[m			[32mhours=((to-ti).total_seconds())/3600[m
[32m+[m			[32mobj.hours=hours[m
[32m+[m[41m		[m
[32m+[m
[32m+[m		[32melse:[m
[32m+[m			[32mobj.hours=0[m
[32m+[m
[32m+[m		[32m(check,break_hourss)= check_validity_times(times_all)[m
[32m+[m		[32mif check:[m
[32m+[m			[32mobj.break_hours=break_hourss[m
[32m+[m
[32m+[m
[32m+[m		[32melse:[m
[32m+[m			[32mobj.break_hours=0[m
[32m+[m
[32m+[m
[32m+[m[41m		[m
[32m+[m		[32mdf_hours.append(obj.hours)[m
[32m+[m		[32mdf_break_hours.append(obj.break_hours)[m
[32m+[m		[32mobj.hours=convert_hours_to_hours_mins(obj.hours)[m
[32m+[m		[32mobj.break_hours=convert_hours_to_hours_mins(obj.break_hours)[m
[32m+[m[41m			[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m	[32mdf = read_frame(qs)[m[41m	[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m	[32mdf["hours"]=df_hours[m
[32m+[m	[32mdf["break_hours"]=df_break_hours[m
[32m+[m
[32m+[m	[32mprint(df)[m
[32m+[m[41m	[m
[32m+[m	[32msns.barplot(data=df,x='date',y='hours')[m
[32m+[m	[32mplt.xticks(rotation='vertical')[m
[32m+[m	[32mrcParams.update({'figure.autolayout': True})[m
[32m+[m	[32mplt.tight_layout()[m
[32m+[m	[32mif(admin):[m
[32m+[m		[32mplt.savefig('./recognition/static/recognition/img/attendance_graphs/hours_vs_date/1.png')[m
[32m+[m		[32mplt.close()[m
[32m+[m	[32melse:[m
[32m+[m		[32mplt.savefig('./recognition/static/recognition/img/attendance_graphs/employee_login/1.png')[m
[32m+[m		[32mplt.close()[m
[32m+[m	[32mreturn qs[m
[32m+[m[41m	[m
[32m+[m
[32m+[m[32m#used[m
[32m+[m[32mdef hours_vs_employee_given_date(present_qs,time_qs):[m
[32m+[m	[32mregister_matplotlib_converters()[m
[32m+[m	[32mdf_hours=[][m
[32m+[m	[32mdf_break_hours=[][m
[32m+[m	[32mdf_username=[][m
[32m+[m	[32mqs=present_qs[m
[32m+[m
[32m+[m	[32mfor obj in qs:[m
[32m+[m		[32muser=obj.user[m
[32m+[m		[32mtimes_in=time_qs.filter(user=user).filter(out=False)[m
[32m+[m		[32mtimes_out=time_qs.filter(user=user).filter(out=True)[m
[32m+[m		[32mtimes_all=time_qs.filter(user=user)[m
[32m+[m		[32mobj.time_in=None[m
[32m+[m		[32mobj.time_out=None[m
[32m+[m		[32mobj.hours=0[m
[32m+[m		[32mobj.hours=0[m
[32m+[m		[32mif (len(times_in)>0):[m[41m			[m
[32m+[m			[32mobj.time_in=times_in.first().time[m
[32m+[m		[32mif (len(times_out)>0):[m
[32m+[m			[32mobj.time_out=times_out.last().time[m
[32m+[m		[32mif(obj.time_in is not None and obj.time_out is not None):[m
[32m+[m			[32mti=obj.time_in[m
[32m+[m			[32mto=obj.time_out[m
[32m+[m			[32mhours=((to-ti).total_seconds())/3600[m
[32m+[m			[32mobj.hours=hours[m
[32m+[m		[32melse:[m
[32m+[m			[32mobj.hours=0[m
[32m+[m		[32m(check,break_hourss)= check_validity_times(times_all)[m
[32m+[m		[32mif check:[m
[32m+[m			[32mobj.break_hours=break_hourss[m
[32m+[m
[32m+[m
[32m+[m		[32melse:[m
[32m+[m			[32mobj.break_hours=0[m
[32m+[m
[32m+[m[41m		[m
[32m+[m		[32mdf_hours.append(obj.hours)[m
[32m+[m		[32mdf_username.append(user.username)[m
[32m+[m		[32mdf_break_hours.append(obj.break_hours)[m
[32m+[m		[32mobj.hours=convert_hours_to_hours_mins(obj.hours)[m
[32m+[m		[32mobj.break_hours=convert_hours_to_hours_mins(obj.break_hours)[m
[32m+[m
[32m+[m[41m	[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m	[32mdf = read_frame(qs)[m[41m	[m
[32m+[m	[32mdf['hours']=df_hours[m
[32m+[m	[32mdf['username']=df_username[m
[32m+[m	[32mdf["break_hours"]=df_break_hours[m
[32m+[m
[32m+[m
[32m+[m	[32msns.barplot(data=df,x='username',y='hours')[m
[32m+[m	[32mplt.xticks(rotation='vertical')[m
[32m+[m	[32mrcParams.update({'figure.autolayout': True})[m
[32m+[m	[32mplt.tight_layout()[m
[32m+[m	[32mplt.savefig('./recognition/static/recognition/img/attendance_graphs/hours_vs_employee/1.png')[m
[32m+[m	[32mplt.close()[m
[32m+[m	[32mreturn qs[m
[32m+[m
[32m+[m
[32m+[m[32mdef total_number_employees():[m
[32m+[m	[32mqs=User.objects.all()[m
[32m+[m	[32mreturn (len(qs) -1)[m
[32m+[m	[32m# -1 to account for admin[m[41m [m
[32m+[m
