[32m+[m[32mdef employees_present_today():[m
[32m+[m	[32mtoday=datetime.date.today()[m
[32m+[m	[32mqs=Present.objects.filter(date=today).filter(present=True)[m
[32m+[m	[32mreturn len(qs)[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m#used[m[41m	[m
[32m+[m[32mdef this_week_emp_count_vs_date():[m
[32m+[m	[32mtoday=datetime.date.today()[m
[32m+[m	[32msome_day_last_week=today-datetime.timedelta(days=7)[m
[32m+[m	[32mmonday_of_last_week=some_day_last_week-  datetime.timedelta(days=(some_day_last_week.isocalendar()[2] - 1))[m
[32m+[m	[32mmonday_of_this_week = monday_of_last_week + datetime.timedelta(days=7)[m
[32m+[m	[32mqs=Present.objects.filter(date__gte=monday_of_this_week).filter(date__lte=today)[m
[32m+[m	[32mstr_dates=[][m
[32m+[m	[32memp_count=[][m
[32m+[m	[32mstr_dates_all=[][m
[32m+[m	[32memp_cnt_all=[][m
[32m+[m	[32mcnt=0[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m	[32mfor obj in qs:[m
[32m+[m		[32mdate=obj.date[m
[32m+[m		[32mstr_dates.append(str(date))[m
[32m+[m		[32mqs=Present.objects.filter(date=date).filter(present=True)[m
[32m+[m		[32memp_count.append(len(qs))[m
[32m+[m
[32m+[m
[32m+[m	[32mwhile(cnt<5):[m
[32m+[m
[32m+[m		[32mdate=str(monday_of_this_week+datetime.timedelta(days=cnt))[m
[32m+[m		[32mcnt+=1[m
[32m+[m		[32mstr_dates_all.append(date)[m
[32m+[m		[32mif(str_dates.count(date))>0:[m
[32m+[m			[32midx=str_dates.index(date)[m
[32m+[m
[32m+[m			[32memp_cnt_all.append(emp_count[idx])[m
[32m+[m		[32melse:[m
[32m+[m			[32memp_cnt_all.append(0)[m
[32m+[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m	[32mdf=pd.DataFrame()[m
[32m+[m	[32mdf["date"]=str_dates_all[m
[32m+[m	[32mdf["Number of employees"]=emp_cnt_all[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m	[32msns.lineplot(data=df,x='date',y='Number of employees')[m
[32m+[m	[32mplt.savefig('./recognition/static/recognition/img/attendance_graphs/this_week/1.png')[m
[32m+[m	[32mplt.close()[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
