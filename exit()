[32m+[m[32mfrom django.shortcuts import render,redirect[m
[32m+[m[32mfrom django.contrib.auth.forms import UserCreationForm[m
[32m+[m[32mfrom django.contrib import messages[m
[32m+[m[32mfrom django.contrib.auth.decorators import login_required[m
[32m+[m
[32m+[m
[32m+[m[32m#utility functions[m
[32m+[m[32m'''[m
[32m+[m[32mdef hours_vs_date_every_employee():[m
[32m+[m	[32mqs = Attendance.objects.all()[m
[32m+[m	[32mdiff=[][m
[32m+[m[41m	[m
[32m+[m	[32mfor obj in qs:[m
[32m+[m		[32mti=obj.time_in[m
[32m+[m		[32mto=obj.time_out[m
[32m+[m		[32mhours=((to-ti).total_seconds())/3600[m
[32m+[m		[32mdiff.append(hours)[m
[32m+[m[41m		[m
[32m+[m	[32mdf = read_frame(qs)[m
[32m+[m	[32mdf['hours']=diff[m
[32m+[m	[32mfigure=plt.figure()[m
[32m+[m	[32msns.barplot(data=df,x='date',y='')[m
[32m+[m	[32mhtml_graph=mpld3.fig_to_html(fig)[m
[32m+[m
[32m+[m
[32m+[m[32m'''[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m# Create your views here.[m
[32m+[m
[32m+[m[32m@login_required[m
[32m+[m[32mdef register(request):[m
[32m+[m	[32mif request.user.username!='admin':[m
[32m+[m		[32mreturn redirect('not-authorised')[m
[32m+[m	[32mif request.method=='POST':[m
[32m+[m		[32mform=UserCreationForm(request.POST)[m
[32m+[m		[32mif form.is_valid():[m
[32m+[m			[32mform.save() ###add user to database[m
[32m+[m			[32mmessages.success(request, f'Employee registered successfully!')[m
[32m+[m			[32mreturn redirect('dashboard')[m
[32m+[m[41m		[m
[32m+[m
[32m+[m
[32m+[m	[32melse:[m
[32m+[m		[32mform=UserCreationForm()[m
[32m+[m	[32mreturn render(request,'users/register.html', {'form' : form})[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
