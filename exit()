[32m+[m			[32mdate=form.cleaned_data.get('date')[m
[32m+[m			[32mprint("date:"+ str(date))[m
[32m+[m			[32mtime_qs=Time.objects.filter(date=date)[m
[32m+[m			[32mpresent_qs=Present.objects.filter(date=date)[m
[32m+[m			[32mif(len(time_qs)>0 or len(present_qs)>0):[m
[32m+[m				[32mqs=hours_vs_employee_given_date(present_qs,time_qs)[m
[32m+[m
[32m+[m
[32m+[m				[32mreturn render(request,'recognition/view_attendance_date.html', {'form' : form,'qs' : qs })[m
