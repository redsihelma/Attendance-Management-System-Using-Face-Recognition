[32m+[m
[32m+[m[32m@login_required[m
[32m+[m[32mdef train(request):[m
[32m+[m	[32mif request.user.username!='admin':[m
[32m+[m		[32mreturn redirect('not-authorised')[m
[32m+[m
[32m+[m	[32mtraining_dir='face_recognition_data/training_dataset'[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m	[32mcount=0[m
[32m+[m	[32mfor person_name in os.listdir(training_dir):[m
[32m+[m		[32mcurr_directory=os.path.join(training_dir,person_name)[m
[32m+[m		[32mif not os.path.isdir(curr_directory):[m
[32m+[m			[32mcontinue[m
[32m+[m		[32mfor imagefile in image_files_in_folder(curr_directory):[m
[32m+[m			[32mcount+=1[m
[32m+[m
[32m+[m	[32mX=[][m
[32m+[m	[32my=[][m
[32m+[m	[32mi=0[m
[32m+[m
[32m+[m
[32m+[m	[32mfor person_name in os.listdir(training_dir):[m
[32m+[m		[32mprint(str(person_name))[m
[32m+[m		[32mcurr_directory=os.path.join(training_dir,person_name)[m
[32m+[m		[32mif not os.path.isdir(curr_directory):[m
[32m+[m			[32mcontinue[m
[32m+[m		[32mfor imagefile in image_files_in_folder(curr_directory):[m
[32m+[m			[32mprint(str(imagefile))[m
[32m+[m			[32mimage=cv2.imread(imagefile)[m
[32m+[m			[32mtry:[m
[32m+[m				[32mX.append((face_recognition.face_encodings(image)[0]).tolist())[m
[32m+[m[41m				[m
[32m+[m
[32m+[m[41m				[m
[32m+[m				[32my.append(person_name)[m
[32m+[m				[32mi+=1[m
[32m+[m			[32mexcept:[m
[32m+[m				[32mprint("removed")[m
[32m+[m				[32mos.remove(imagefile)[m
[32m+[m
[32m+[m[41m			[m
[32m+[m
[32m+[m
[32m+[m	[32mtargets=np.array(y)[m
[32m+[m	[32mencoder = LabelEncoder()[m
[32m+[m	[32mencoder.fit(y)[m
[32m+[m	[32my=encoder.transform(y)[m
[32m+[m	[32mX1=np.array(X)[m
[32m+[m	[32mprint("shape: "+ str(X1.shape))[m
[32m+[m	[32mnp.save('face_recognition_data/classes.npy', encoder.classes_)[m
[32m+[m	[32msvc = SVC(kernel='linear',probability=True)[m
[32m+[m	[32msvc.fit(X1,y)[m
[32m+[m	[32msvc_save_path="face_recognition_data/svc.sav"[m
[32m+[m	[32mwith open(svc_save_path, 'wb') as f:[m
[32m+[m		[32mpickle.dump(svc,f)[m
[32m+[m
[32m+[m[41m	[m
[32m+[m	[32mvizualize_Data(X1,targets)[m
[32m+[m[41m	[m
[32m+[m	[32mmessages.success(request, f'Training Complete.')[m
[32m+[m
[32m+[m	[32mreturn render(request,"recognition/train.html")[m
[32m+[m
