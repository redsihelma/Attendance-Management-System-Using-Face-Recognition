[32m+[m
[32m+[m[32mdef mark_your_attendance(request):[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m
[32m+[m[41m	[m
[32m+[m	[32mdetector = dlib.get_frontal_face_detector()[m
[32m+[m[41m	[m
[32m+[m	[32mpredictor = dlib.shape_predictor('face_recognition_data/shape_predictor_68_face_landmarks.dat')   #Add path to the shape predictor ######CHANGE TO RELATIVE PATH LATER[m
[32m+[m	[32msvc_save_path="face_recognition_data/svc.sav"[m[41m	[m
[32m+[m
[32m+[m
[32m+[m[41m		[m
[32m+[m[41m			[m
[32m+[m	[32mwith open(svc_save_path, 'rb') as f:[m
[32m+[m			[32msvc = pickle.load(f)[m
[32m+[m	[32mfa = FaceAligner(predictor , desiredFaceWidth = 96)[m
[32m+[m	[32mencoder=LabelEncoder()[m
[32m+[m	[32mencoder.classes_ = np.load('face_recognition_data/classes.npy')[m
[32m+[m
[32m+[m
[32m+[m	[32mfaces_encodings = np.zeros((1,128))[m
[32m+[m	[32mno_of_faces = len(svc.predict_proba(faces_encodings)[0])[m
[32m+[m	[32mcount = dict()[m
[32m+[m	[32mpresent = dict()[m
[32m+[m	[32mlog_time = dict()[m
[32m+[m	[32mstart = dict()[m
[32m+[m	[32mfor i in range(no_of_faces):[m
[32m+[m		[32mcount[encoder.inverse_transform([i])[0]] = 0[m
[32m+[m		[32mpresent[encoder.inverse_transform([i])[0]] = False[m
[32m+[m
[32m+[m[41m	[m
[32m+[m
[32m+[m	[32mvs = VideoStream(src=0).start()[m
[32m+[m[41m	[m
[32m+[m	[32msampleNum = 0[m
[32m+[m[41m	[m
[32m+[m	[32mwhile(True):[m
[32m+[m[41m		[m
[32m+[m		[32mframe = vs.read()[m
[32m+[m[41m		[m
[32m+[m		[32mframe = imutils.resize(frame ,width = 800)[m
[32m+[m[41m		[m
[32m+[m		[32mgray_frame = cv2.cvtColor(frame, cv2.COLOR_BGR2GRAY)[m
[32m+[m[41m		[m
[32m+[m		[32mfaces = detector(gray_frame,0)[m
[32m+[m[41m		[m
[32m+[m[41m		[m
[32m+[m
[32m+[m
[32m+[m		[32mfor face in faces:[m
[32m+[m			[32mprint("INFO : inside for loop")[m
[32m+[m			[32m(x,y,w,h) = face_utils.rect_to_bb(face)[m
[32m+[m
[32m+[m			[32mface_aligned = fa.align(frame,gray_frame,face)[m
[32m+[m			[32mcv2.rectangle(frame,(x,y),(x+w,y+h),(0,255,0),1)[m
[32m+[m[41m					[m
[32m+[m[41m			[m
[32m+[m			[32m(pred,prob)=predict(face_aligned,svc)[m
[32m+[m[41m			[m
[32m+[m
[32m+[m[41m			[m
[32m+[m			[32mif(pred!=[-1]):[m
[32m+[m[41m				[m
[32m+[m				[32mperson_name=encoder.inverse_transform(np.ravel([pred]))[0][m
[32m+[m				[32mpred=person_name[m
[32m+[m				[32mif count[pred] == 0:[m
[32m+[m					[32mstart[pred] = time.time()[m
[32m+[m					[32mcount[pred] = count.get(pred,0) + 1[m
[32m+[m
[32m+[m				[32mif count[pred] == 4 and (time.time()-start[pred]) > 1.2:[m
[32m+[m					[32mcount[pred] = 0[m
[32m+[m				[32melse:[m
[32m+[m				[32m#if count[pred] == 4 and (time.time()-start) <= 1.5:[m
[32m+[m					[32mpresent[pred] = True[m
[32m+[m					[32mlog_time[pred] = datetime.datetime.now()[m
[32m+[m					[32mcount[pred] = count.get(pred,0) + 1[m
[32m+[m					[32mprint(pred, present[pred], count[pred])[m
[32m+[m				[32mcv2.putText(frame, str(person_name)+ str(prob), (x+6,y+h-6), cv2.FONT_HERSHEY_SIMPLEX,0.5,(0,255,0),1)[m
[32m+[m
[32m+[m			[32melse:[m
[32m+[m				[32mperson_name="unknown"[m
[32m+[m				[32mcv2.putText(frame, str(person_name), (x+6,y+h-6), cv2.FONT_HERSHEY_SIMPLEX,0.5,(0,255,0),1)[m
[32m+[m
[32m+[m[41m			[m
[32m+[m[41m			[m
[32m+[m			[32m#cv2.putText()[m
[32m+[m			[32m# Before continuing to the next loop, I want to give it a little pause[m
[32m+[m			[32m# waitKey of 100 millisecond[m
[32m+[m			[32m#cv2.waitKey(50)[m
[32m+[m
[32m+[m		[32m#Showing the image in another window[m
[32m+[m		[32m#Creates a window with window name "Face" and with the image img[m
[32m+[m		[32mcv2.imshow("Mark Attendance - In - Press q to exit",frame)[m
[32m+[m		[32m#Before closing it we need to give a wait command, otherwise the open cv wont work[m
[32m+[m		[32m# @params with the millisecond of delay 1[m
[32m+[m		[32m#cv2.waitKey(1)[m
[32m+[m		[32m#To get out of the loop[m
[32m+[m		[32mkey=cv2.waitKey(50) & 0xFF[m
[32m+[m		[32mif(key==ord("q")):[m
[32m+[m			[32mbreak[m
[32m+[m[41m	[m
[32m+[m	[32m#Stoping the videostream[m
[32m+[m	[32mvs.stop()[m
[32m+[m
[32m+[m	[32m# destroying all the windows[m
[32m+[m	[32mcv2.destroyAllWindows()[m
[32m+[m	[32mupdate_attendance_in_db_in(present)[m
[32m+[m	[32mreturn redirect('home')[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mdef mark_your_attendance_out(request):[m
[32m+[m[41m	[m
[32m+[m
[32m+[m[41m	[m
[32m+[m	[32mdetector = dlib.get_frontal_face_detector()[m
[32m+[m[41m	[m
[32m+[m	[32mpredictor = dlib.shape_predictor('face_recognition_data/shape_predictor_68_face_landmarks.dat')   #Add path to the shape predictor ######CHANGE TO RELATIVE PATH LATER[m
[32m+[m	[32msvc_save_path="face_recognition_data/svc.sav"[m[41m	[m
[32m+[m
[32m+[m
[32m+[m[41m		[m
[32m+[m[41m			[m
[32m+[m	[32mwith open(svc_save_path, 'rb') as f:[m
[32m+[m			[32msvc = pickle.load(f)[m
[32m+[m	[32mfa = FaceAligner(predictor , desiredFaceWidth = 96)[m
[32m+[m	[32mencoder=LabelEncoder()[m
[32m+[m	[32mencoder.classes_ = np.load('face_recognition_data/classes.npy')[m
[32m+[m
[32m+[m
[32m+[m	[32mfaces_encodings = np.zeros((1,128))[m
[32m+[m	[32mno_of_faces = len(svc.predict_proba(faces_encodings)[0])[m
[32m+[m	[32mcount = dict()[m
[32m+[m	[32mpresent = dict()[m
[32m+[m	[32mlog_time = dict()[m
[32m+[m	[32mstart = dict()[m
[32m+[m	[32mfor i in range(no_of_faces):[m
[32m+[m		[32mcount[encoder.inverse_transform([i])[0]] = 0[m
[32m+[m		[32mpresent[encoder.inverse_transform([i])[0]] = False[m
[32m+[m
[32m+[m[41m	[m
[32m+[m
[32m+[m	[32mvs = VideoStream(src=0).start()[m
[32m+[m[41m	[m
[32m+[m	[32msampleNum = 0[m
[32m+[m[41m	[m
[32m+[m	[32mwhile(True):[m
[32m+[m[41m		[m
[32m+[m		[32mframe = vs.read()[m
[32m+[m[41m		[m
[32m+[m		[32mframe = imutils.resize(frame ,width = 800)[m
[32m+[m[41m		[m
[32m+[m		[32mgray_frame = cv2.cvtColor(frame, cv2.COLOR_BGR2GRAY)[m
[32m+[m[41m		[m
[32m+[m		[32mfaces = detector(gray_frame,0)[m
[32m+[m[41m		[m
[32m+[m[41m		[m
[32m+[m
[32m+[m
[32m+[m		[32mfor face in faces:[m
[32m+[m			[32mprint("INFO : inside for loop")[m
[32m+[m			[32m(x,y,w,h) = face_utils.rect_to_bb(face)[m
[32m+[m
[32m+[m			[32mface_aligned = fa.align(frame,gray_frame,face)[m
[32m+[m			[32mcv2.rectangle(frame,(x,y),(x+w,y+h),(0,255,0),1)[m
[32m+[m[41m					[m
[32m+[m[41m			[m
[32m+[m			[32m(pred,prob)=predict(face_aligned,svc)[m
[32m+[m[41m			[m
[32m+[m
[32m+[m[41m			[m
[32m+[m			[32mif(pred!=[-1]):[m
[32m+[m[41m				[m
[32m+[m				[32mperson_name=encoder.inverse_transform(np.ravel([pred]))[0][m
[32m+[m				[32mpred=person_name[m
[32m+[m				[32mif count[pred] == 0:[m
[32m+[m					[32mstart[pred] = time.time()[m
[32m+[m					[32mcount[pred] = count.get(pred,0) + 1[m
[32m+[m
[32m+[m				[32mif count[pred] == 4 and (time.time()-start[pred]) > 1.5:[m
[32m+[m					[32mcount[pred] = 0[m
[32m+[m				[32melse:[m
[32m+[m				[32m#if count[pred] == 4 and (time.time()-start) <= 1.5:[m
[32m+[m					[32mpresent[pred] = True[m
[32m+[m					[32mlog_time[pred] = datetime.datetime.now()[m
[32m+[m					[32mcount[pred] = count.get(pred,0) + 1[m
[32m+[m					[32mprint(pred, present[pred], count[pred])[m
[32m+[m				[32mcv2.putText(frame, str(person_name)+ str(prob), (x+6,y+h-6), cv2.FONT_HERSHEY_SIMPLEX,0.5,(0,255,0),1)[m
[32m+[m
[32m+[m			[32melse:[m
[32m+[m				[32mperson_name="unknown"[m
[32m+[m				[32mcv2.putText(frame, str(person_name), (x+6,y+h-6), cv2.FONT_HERSHEY_SIMPLEX,0.5,(0,255,0),1)[m
[32m+[m
[32m+[m[41m			[m
[32m+[m[41m			[m
[32m+[m			[32m#cv2.putText()[m
[32m+[m			[32m# Before continuing to the next loop, I want to give it a little pause[m
[32m+[m			[32m# waitKey of 100 millisecond[m
[32m+[m			[32m#cv2.waitKey(50)[m
[32m+[m
[32m+[m		[32m#Showing the image in another window[m
[32m+[m		[32m#Creates a window with window name "Face" and with the image img[m
[32m+[m		[32mcv2.imshow("Mark Attendance- Out - Press q to exit",frame)[m
[32m+[m		[32m#Before closing it we need to give a wait command, otherwise the open cv wont work[m
[32m+[m		[32m# @params with the millisecond of delay 1[m
[32m+[m		[32m#cv2.waitKey(1)[m
[32m+[m		[32m#To get out of the loop[m
[32m+[m		[32mkey=cv2.waitKey(50) & 0xFF[m
[32m+[m		[32mif(key==ord("q")):[m
[32m+[m			[32mbreak[m
[32m+[m[41m	[m
[32m+[m	[32m#Stoping the videostream[m
[32m+[m	[32mvs.stop()[m
[32m+[m
[32m+[m	[32m# destroying all the windows[m
[32m+[m	[32mcv2.destroyAllWindows()[m
[32m+[m	[32mupdate_attendance_in_db_out(present)[m
[32m+[m	[32mreturn redirect('home')[m
