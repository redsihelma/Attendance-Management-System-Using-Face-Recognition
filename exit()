[32m+[m[32mdef create_dataset(username):[m
[32m+[m	[32mid = username[m
[32m+[m	[32mif(os.path.exists('face_recognition_data/training_dataset/{}/'.format(id))==False):[m
[32m+[m		[32mos.makedirs('face_recognition_data/training_dataset/{}/'.format(id))[m
[32m+[m	[32mdirectory='face_recognition_data/training_dataset/{}/'.format(id)[m
[32m+[m
[32m+[m	[32m# Detect face[m
[32m+[m	[32m#Loading the HOG face detector and the shape predictpr for allignment[m
[32m+[m
[32m+[m	[32mprint("[INFO] Loading the facial detector")[m
[32m+[m	[32mdetector = dlib.get_frontal_face_detector()[m
[32m+[m	[32mpredictor = dlib.shape_predictor('face_recognition_data/shape_predictor_68_face_landmarks.dat')   #Add path to the shape predictor ######CHANGE TO RELATIVE PATH LATER[m
[32m+[m	[32mfa = FaceAligner(predictor , desiredFaceWidth = 96)[m
[32m+[m	[32m#capture images from the webcam and process and detect the face[m
[32m+[m	[32m# Initialize the video stream[m
[32m+[m	[32mprint("[INFO] Initializing Video stream")[m
[32m+[m	[32mvs = VideoStream(src=0).start()[m
[32m+[m	[32m#time.sleep(2.0) ####CHECK######[m
[32m+[m
[32m+[m	[32m# Our identifier[m
[32m+[m	[32m# We will put the id here and we will store the id with a face, so that later we can identify whose face it is[m
[32m+[m[41m	[m
[32m+[m	[32m# Our dataset naming counter[m
[32m+[m	[32msampleNum = 0[m
[32m+[m	[32m# Capturing the faces one by one and detect the faces and showing it on the window[m
[32m+[m	[32mwhile(True):[m
[32m+[m		[32m# Capturing the image[m
[32m+[m		[32m#vs.read each frame[m
[32m+[m		[32mframe = vs.read()[m
[32m+[m		[32m#Resize each image[m
[32m+[m		[32mframe = imutils.resize(frame ,width = 800)[m
[32m+[m		[32m#the returned img is a colored image but for the classifier to work we need a greyscale image[m
[32m+[m		[32m#to convert[m
[32m+[m		[32mgray_frame = cv2.cvtColor(frame, cv2.COLOR_BGR2GRAY)[m
[32m+[m		[32m#To store the faces[m
[32m+[m		[32m#This will detect all the images in the current frame, and it will return the coordinates of the faces[m
[32m+[m		[32m#Takes in image and some other parameter for accurate result[m
[32m+[m		[32mfaces = detector(gray_frame,0)[m
[32m+[m		[32m#In above 'faces' variable there can be multiple faces so we have to get each and every face and draw a rectangle around it.[m
[32m+[m[41m		[m
[32m+[m[41m		[m
[32m+[m[41m			[m
[32m+[m
[32m+[m
[32m+[m		[32mfor face in faces:[m
[32m+[m			[32mprint("inside for loop")[m
[32m+[m			[32m(x,y,w,h) = face_utils.rect_to_bb(face)[m
[32m+[m
[32m+[m			[32mface_aligned = fa.align(frame,gray_frame,face)[m
[32m+[m			[32m# Whenever the program captures the face, we will write that is a folder[m
[32m+[m			[32m# Before capturing the face, we need to tell the script whose face it is[m
[32m+[m			[32m# For that we will need an identifier, here we call it id[m
[32m+[m			[32m# So now we captured a face, we need to write it in a file[m
[32m+[m			[32msampleNum = sampleNum+1[m
[32m+[m			[32m# Saving the image dataset, but only the face part, cropping the rest[m
[32m+[m[41m			[m
[32m+[m			[32mif face is None:[m
[32m+[m				[32mprint("face is none")[m
[32m+[m				[32mcontinue[m
[32m+[m
[32m+[m
[32m+[m[41m			[m
[32m+[m
[32m+[m			[32mcv2.imwrite(directory+'/'+str(sampleNum)+'.jpg'	, face_aligned)[m
[32m+[m			[32mface_aligned = imutils.resize(face_aligned ,width = 400)[m
[32m+[m			[32m#cv2.imshow("Image Captured",face_aligned)[m
[32m+[m			[32m# @params the initial point of the rectangle will be x,y and[m
[32m+[m			[32m# @params end point will be x+width and y+height[m
[32m+[m			[32m# @params along with color of the rectangle[m
[32m+[m			[32m# @params thickness of the rectangle[m
[32m+[m			[32mcv2.rectangle(frame,(x,y),(x+w,y+h),(0,255,0),1)[m
[32m+[m			[32m# Before continuing to the next loop, I want to give it a little pause[m
[32m+[m			[32m# waitKey of 100 millisecond[m
[32m+[m			[32mcv2.waitKey(50)[m
[32m+[m
[32m+[m		[32m#Showing the image in another window[m
[32m+[m		[32m#Creates a window with window name "Face" and with the image img[m
[32m+[m		[32mcv2.imshow("Add Images",frame)[m
[32m+[m		[32m#Before closing it we need to give a wait command, otherwise the open cv wont work[m
[32m+[m		[32m# @params with the millisecond of delay 1[m
[32m+[m		[32mcv2.waitKey(1)[m
[32m+[m		[32m#To get out of the loop[m
[32m+[m		[32mif(sampleNum>99):[m
[32m+[m			[32mbreak[m
[32m+[m[41m	[m
[32m+[m	[32m#Stoping the videostream[m
[32m+[m	[32mvs.stop()[m
[32m+[m	[32m# destroying all the windows[m
[32m+[m	[32mcv2.destroyAllWindows()[m
[32m+[m
[32m+[m
[32m+[m[32mdef predict(face_aligned,svc,threshold=0.7):[m
[32m+[m	[32mface_encodings=np.zeros((1,128))[m
[32m+[m	[32mtry:[m
[32m+[m		[32mx_face_locations=face_recognition.face_locations(face_aligned)[m
[32m+[m		[32mfaces_encodings=face_recognition.face_encodings(face_aligned,known_face_locations=x_face_locations)[m
[32m+[m		[32mif(len(faces_encodings)==0):[m
[32m+[m			[32mreturn ([-1],[0])[m
[32m+[m
[32m+[m	[32mexcept:[m
[32m+[m
[32m+[m		[32mreturn ([-1],[0])[m
[32m+[m
[32m+[m	[32mprob=svc.predict_proba(faces_encodings)[m
[32m+[m	[32mresult=np.where(prob[0]==np.amax(prob[0]))[m
[32m+[m	[32mif(prob[0][result[0]]<=threshold):[m
[32m+[m		[32mreturn ([-1],prob[0][result[0]])[m
[32m+[m
[32m+[m	[32mreturn (result[0],prob[0][result[0]])[m
[32m+[m
[32m+[m
[32m+[m[32mdef vizualize_Data(embedded, targets,):[m
[32m+[m[41m	[m
[32m+[m	[32mX_embedded = TSNE(n_components=2).fit_transform(embedded)[m
[32m+[m
[32m+[m	[32mfor i, t in enumerate(set(targets)):[m
[32m+[m		[32midx = targets == t[m
[32m+[m		[32mplt.scatter(X_embedded[idx, 0], X_embedded[idx, 1], label=t)[m
[32m+[m
[32m+[m	[32mplt.legend(bbox_to_anchor=(1, 1));[m
[32m+[m	[32mrcParams.update({'figure.autolayout': True})[m
[32m+[m	[32mplt.tight_layout()[m[41m	[m
[32m+[m	[32mplt.savefig('./recognition/static/recognition/img/training_visualisation.png')[m
[32m+[m	[32mplt.close()[m
